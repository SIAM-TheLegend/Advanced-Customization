    },
    "Supabase": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@supabase/mcp-server-supabase@latest", "--access-token", "sbp_0b5e82caf64605b4bc3ef9b43517bd575eed5d02"]
